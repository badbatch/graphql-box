{
  "name": "@handl/cli",
  "version": "0.1.22",
  "description": "The Handl CLI module.",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/badbatch/handl",
  "bugs": {
    "url": "https://github.com/badbatch/handl/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/badbatch/handl"
  },
  "main": "lib/main/index.js",
  "types": "lib/types/index.d.ts",
  "keywords": [
    "handl",
    "graphql",
    "client",
    "server",
    "isomorphic"
  ],
  "bin": {
    "introspect": "bin/introspect"
  },
  "scripts": {
    "compile": "concurrently npm:compile:*",
    "compile:main": "cross-env BABEL_ENV=main babel ./src --out-dir ./lib/main --extensions \".ts\" --config-file ../../babel.config.js",
    "compile:types": "tsc --build",
    "docs": "typedoc --includes ./src/introspect --out ./docs --options ../../typedoc.js"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.0.0",
    "core-js": "^2.0",
    "graphql": "^14.1.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5"
  },
  "dependencies": {
    "@handl/core": "^0.1.22",
    "fs-extra": "^7.0.1",
    "shelljs": "^0.8.3",
    "yargs": "^13.2.0"
  }
}
