{
  "name": "@graphql-box/gql.macro",
  "description": "The GraphQL Box graphql file import module.",
  "version": "5.4.3",
  "author": "miami-man",
  "license": "MIT",
  "homepage": "https://github.com/badbatch/graphql-box",
  "repository": {
    "directory": "packages/gql.macro",
    "type": "git",
    "url": "https://github.com/badbatch/graphql-box"
  },
  "bugs": "https://github.com/badbatch/graphql-box/issues",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./index.d.ts",
  "imports": {
    "#*": "./src/*"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean:dist && pnpm run compile",
    "clean:deps": "del-cli ./node_modules",
    "clean:dist": "del-cli ./dist",
    "compile": "pnpm run /^compile:.*/",
    "compile:cjs": "MODULE_SYSTEM=cjs rollup -c ../../rollup.config.babel.cjs",
    "compile:esm": "rollup -c ../../rollup.config.babel.cjs"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@babel/types": "^7.26.3",
    "@types/babel__core": "^7.20.5",
    "@types/babel-plugin-macros": "^3.1.3",
    "babel-plugin-macros": "^3.1.0",
    "core-js": "^3.39.0",
    "ts-md5": "^1.3.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "cts-types": "^0.0.8",
    "del-cli": "^6.0.0"
  },
  "keywords": [
    "client",
    "graphql",
    "graphql-box",
    "isomorphic",
    "server"
  ]
}
