{
  "name": "@graphql-box/gql.macro",
  "description": "The GraphQL Box graphql file import module.",
  "version": "4.1.3",
  "author": "Dylan Aubrey",
  "license": "MIT",
  "homepage": "https://github.com/badbatch/graphql-box",
  "repository": {
    "directory": "packages/gql.macro",
    "type": "git",
    "url": "https://github.com/badbatch/graphql-box"
  },
  "bugs": "https://github.com/badbatch/graphql-box/issues",
  "type": "module",
  "main": "./dist/main/index.mjs",
  "types": "./dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean:dist && pnpm run compile",
    "clean:deps": "del-cli ./node_modules",
    "clean:dist": "del-cli ./dist",
    "compile": "pnpm run /^compile:.*/",
    "compile:main": "rollup -c ../../rollup.config.cjs",
    "compile:types": "tsc --project ./tsconfig.build.json"
  },
  "dependencies": {
    "@babel/core": "^7.8.6",
    "@babel/types": "^7.8.6",
    "@graphql-box/core": "workspace:../core",
    "@graphql-box/helpers": "workspace:../helpers",
    "babel-plugin-macros": "^3.1.0"
  },
  "peerDependencies": {
    "@babel/runtime": "<8",
    "core-js": "<4",
    "lodash-es": "<5"
  },
  "devDependencies": {
    "@babel/runtime": "^7.20.13",
    "@types/lodash-es": "^4.14.191",
    "core-js": "^3.27.2",
    "lodash-es": "^4.17.21"
  },
  "keywords": [
    "graphql-box",
    "graphql",
    "client",
    "server",
    "isomorphic"
  ]
}
