{
  "name": "@graphql-box/gql.macro",
  "description": "The GraphQL Box graphql file import module.",
  "version": "4.1.3",
  "author": "Dylan Aubrey",
  "license": "MIT",
  "homepage": "https://github.com/badbatch/graphql-box",
  "repository": {
    "directory": "packages/gql.macro",
    "type": "git",
    "url": "https://github.com/badbatch/graphql-box"
  },
  "bugs": "https://github.com/badbatch/graphql-box/issues",
  "type": "commonjs",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/cjs/index.d.cts",
  "exports": {
    "types": {
      "import": "./dist/types/esm/index.d.ts",
      "require": "./dist/types/cjs/index.d.cts"
    },
    "import": "./dist/esm/index.mjs",
    "require": "./dist/cjs/index.cjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean:dist && pnpm run compile",
    "clean:deps": "del-cli ./node_modules",
    "clean:dist": "del-cli ./dist",
    "compile": "pnpm run /^compile:.*/",
    "compile:cjs": "MODULE_SYSTEM=cjs rollup -c ../../rollup.config.cjs",
    "compile:esm": "rollup -c ../../rollup.config.cjs",
    "compile:types": "tsc --project ./tsconfig.build.json && cts-types build dist/types/esm dist/types/cjs"
  },
  "dependencies": {
    "@babel/core": "^7.8.6",
    "@babel/types": "^7.8.6",
    "@types/babel-plugin-macros": "^3.1.3",
    "@types/babel__core": "^7.20.4",
    "babel-plugin-macros": "^3.1.0",
    "ts-md5": "^1.3.1"
  },
  "peerDependencies": {
    "@babel/runtime": "<8",
    "core-js": "<4",
    "lodash-es": "<5"
  },
  "devDependencies": {
    "@babel/runtime": "^7.20.13",
    "@jest/globals": "^29.3.1",
    "@types/lodash-es": "^4.14.191",
    "core-js": "^3.27.2",
    "cts-types": "^0.0.5",
    "del-cli": "^5.1.0",
    "lodash-es": "^4.17.21"
  },
  "keywords": [
    "client",
    "graphql",
    "graphql-box",
    "isomorphic",
    "server"
  ]
}
