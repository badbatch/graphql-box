{
  "name": "@graphql-box/connection-resolver",
  "description": "The GraphQL Box connection manager module.",
  "version": "4.1.4",
  "author": "Dylan Aubrey",
  "license": "MIT",
  "homepage": "https://github.com/badbatch/graphql-box",
  "repository": {
    "directory": "packages/connection-resolver",
    "type": "git",
    "url": "https://github.com/badbatch/graphql-box"
  },
  "bugs": "https://github.com/badbatch/graphql-box/issues",
  "type": "module",
  "main": "./dist/main/index.mjs",
  "types": "./dist/types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm run clean:dist && pnpm run compile",
    "clean:deps": "del-cli ./node_modules",
    "clean:dist": "del-cli ./dist",
    "compile": "pnpm run /^compile:.*/",
    "compile:main": "rollup -c ../../rollup.config.cjs",
    "compile:types": "tsc --project ./tsconfig.build.json"
  },
  "dependencies": {
    "@cachemap/core": "^5.0.2",
    "@graphql-box/core": "workspace:../core",
    "js-base64": "^3.7.5",
    "type-fest": "^4.5.0"
  },
  "peerDependencies": {
    "@babel/runtime": "<8",
    "core-js": "<4",
    "graphql": "<17",
    "lodash-es": "<5"
  },
  "devDependencies": {
    "@babel/runtime": "^7.20.13",
    "@cachemap/map": "^5.0.0",
    "@jest/globals": "^29.3.1",
    "@types/lodash-es": "^4.14.191",
    "core-js": "^3.27.2",
    "graphql": "^16.8.1",
    "lodash-es": "^4.17.21"
  },
  "keywords": [
    "graphql-box",
    "graphql",
    "client",
    "server",
    "isomorphic"
  ]
}
