{
  "name": "graphql-box",
  "description": "An extensible GraphQL client and server with modules for caching, request parsing, subscriptions and more.",
  "version": "5.5.33",
  "author": "Dylan Aubrey",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/badbatch/graphql-box",
  "repository": "badbatch/graphql-box",
  "bugs": "https://github.com/badbatch/graphql-box/issues",
  "type": "module",
  "engines": {
    "node": "^22",
    "pnpm": "^9"
  },
  "scripts": {
    "build": "pnpm run -r build",
    "clean:deps": "pnpm run -r clean:deps && del-cli ./node_modules",
    "clean:dist": "pnpm run -r clean:dist",
    "cut:changelog": "changelog",
    "installActivateMise": "sh shellScripts/installActivateMise.sh",
    "lint": "pnpm run /^lint:.*/",
    "lint:code": "eslint .",
    "lint:docs": "markdownlint-cli2 --config \".markdownlint.json\" \"**/*.md\" \"!**/node_modules/**\"",
    "prepare": "husky",
    "repodog": "repodog",
    "syncpack": "syncpack format && syncpack list-mismatches && syncpack lint-semver-ranges",
    "test": "pnpm run test:node && pnpm run test:browser",
    "test:browser": "pnpm run test:browser:remove && pnpm run test:browser:build && pnpm run test:browser:run",
    "test:browser:build": "webpack --config ./webpack.config.cjs",
    "test:browser:client": "wait-on tcp:3001 && jasmine-browser-runner runSpecs --config=jasmineBrowser.config.cjs --no-random",
    "test:browser:client:debug": "wait-on tcp:3001 && jasmine-browser-runner serve --config=jasmineBrowser.config.cjs --browser chrome",
    "test:browser:debug": "pnpm run test:browser:remove && pnpm run test:browser:build && pnpm run test:browser:run:debug",
    "test:browser:remove": "del-cli ./tests/browser/dist",
    "test:browser:run": "concurrently --kill-others --success first \"npm run test:browser:server\" \"npm run test:browser:client\"",
    "test:browser:run:debug": "concurrently --kill-others --success first \"npm run test:browser:server\" \"npm run test:browser:client:debug\"",
    "test:browser:server": "node --loader ts-node/esm ./tests/server/cli.ts",
    "test:node": "pnpm run /^test:node:.*/",
    "test:node:cjs": "COMPILER=babel BABEL_MODULE_SYSTEM=cjs node node_modules/jest/bin/jest.js --testMatch \"$(pwd)/packages/gql.macro/**/*.test.ts\"",
    "test:node:esm": "node --require=suppress-experimental-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --testMatch \"$(pwd)/packages/!(gql.macro)/**/*.test.ts\"",
    "type-check": "tsc --noEmit",
    "validate": "pnpm run syncpack && pnpm run build && pnpm run lint && pnpm run type-check && pnpm run test"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@cachemap/core": "^5.2.3",
    "@cachemap/core-worker": "^5.2.3",
    "@cachemap/indexed-db": "^5.0.9",
    "@cachemap/map": "^5.0.9",
    "@cachemap/types": "^5.0.9",
    "@commitlint/cli": "^19.5.0",
    "@graphql-box/cache-manager": "workspace:*",
    "@graphql-box/client": "workspace:*",
    "@graphql-box/core": "workspace:*",
    "@graphql-box/debug-manager": "workspace:*",
    "@graphql-box/execute": "workspace:*",
    "@graphql-box/fetch-manager": "workspace:*",
    "@graphql-box/helpers": "workspace:*",
    "@graphql-box/request-parser": "workspace:*",
    "@graphql-box/server": "workspace:*",
    "@graphql-box/subscribe": "workspace:*",
    "@graphql-box/test-utils": "workspace:*",
    "@graphql-box/websocket-manager": "workspace:*",
    "@graphql-box/worker-client": "workspace:*",
    "@graphql-tools/schema": "^10.0.11",
    "@jest/globals": "^29.7.0",
    "@repodog/babel-config": "^7.0.4",
    "@repodog/cli": "^1.6.39",
    "@repodog/commitlint-config": "^19.0.3",
    "@repodog/eslint-config": "^9.0.13",
    "@repodog/eslint-config-jasmine": "^9.0.7",
    "@repodog/eslint-config-jest": "^9.0.4",
    "@repodog/eslint-config-react": "^9.0.4",
    "@repodog/jasmine-browser-config": "^2.0.3",
    "@repodog/jest-config": "^29.0.3",
    "@repodog/markdownlint-config": "^1.2.4",
    "@repodog/prettier-config": "^3.0.2",
    "@repodog/rollup-config": "^4.0.3",
    "@repodog/swc-config": "^1.0.7",
    "@repodog/syncpack-config": "^13.0.2",
    "@repodog/ts-config": "^5.0.4",
    "@repodog/webpack-config": "^5.0.8",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-swc": "^0.3.0",
    "@swc/core": "^1.7.26",
    "@swc/jest": "^0.2.36",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jasmine": "^5.1.5",
    "@types/jest": "^29.5.13",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.5.5",
    "@types/ws": "^8.5.13",
    "babel-loader": "^9.2.1",
    "babel-plugin-macros": "^3.1.0",
    "body-parser": "^1.20.3",
    "cacheability": "^4.0.27",
    "concurrently": "^9.1.0",
    "core-js": "^3.39.0",
    "cors": "^2.8.5",
    "del-cli": "^6.0.0",
    "eslint": "^9.11.0",
    "express": "^4.21.2",
    "fetch-mock": "^11.1.5",
    "generate-changelog": "^1.8.0",
    "graphql": "^16.9.0",
    "husky": "^9.1.6",
    "iterall": "^1.3.0",
    "jasmine-browser-runner": "^2.5.0",
    "jasmine-core": "^5.5.0",
    "jasmine-expect": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lodash-es": "^4.17.15",
    "markdownlint-cli2": "^0.15.0",
    "prettier": "^3.3.3",
    "rollup": "^4.22.4",
    "suppress-experimental-warnings": "^2.0.0",
    "swc-loader": "^0.2.6",
    "syncpack": "^13.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "uuid": "^11.0.3",
    "wait-on": "^8.0.1",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "ws": "^8.18.0"
  },
  "keywords": [
    "client",
    "graphql",
    "graphql-box",
    "isomorphic",
    "server"
  ],
  "pnpm": {
    "overrides": {
      "@babel/helpers": ">=7.26.10",
      "@babel/runtime": ">=7.26.10",
      "axios": ">=1.8.2",
      "braces": ">=3.0.3",
      "cross-spawn": ">=7.0.5",
      "micromatch": ">=4.0.8",
      "nanoid": ">=3.3.8",
      "serialize-javascript": ">=6.0.2"
    }
  }
}
