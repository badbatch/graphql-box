{
  "name": "handl",
  "version": "0.0.28",
  "description": "An isomorphic graphql client and cache.",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/dylanaubrey/handl",
  "bugs": {
    "url": "https://github.com/dylanaubrey/handl/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanaubrey/handl"
  },
  "main": "lib/main/index.js",
  "module": "lib/module/index.js",
  "browser": "lib/browser/index.js",
  "keywords": [
    "graphql",
    "client",
    "isomorphic"
  ],
  "scripts": {
    "build": "npm run clean && npm run test && npm run compile",
    "clean": "gulp clean",
    "compile": "npm run compile:main && npm run compile:module && npm run compile:browser",
    "compile:browser": "gulp browser",
    "compile:main": "gulp main",
    "compile:module": "gulp module",
    "debug:web": "karma start --no-single-run --browsers Chrome",
    "lint": "gulp lint",
    "publish:pkg": "npm run build && npm publish",
    "test": "npm run lint && npm run type-check && npm run test:web && npm run test:node",
    "test:node": "cross-env TS_NODE_PROJECT=\"./tsconfig.test.json\" nyc mocha --opts \"./mocha.opts\"",
    "test:web": "karma start --single-run --browsers PhantomJS",
    "type-check": "gulp type-check"
  },
  "dependencies": {
    "cacheability": "0.0.2",
    "cachemap": "0.0.12",
    "es6-promise": "^4.1.1",
    "graphql": "^0.11.3",
    "iso-logger": "0.0.8",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "md5": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.32",
    "@babel/preset-env": "^7.0.0-beta.32",
    "@babel/preset-stage-0": "^7.0.0-beta.32",
    "@types/chai": "^4.0.8",
    "@types/es6-promise": "0.0.33",
    "@types/graphql": "^0.11.6",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/lodash": "^4.14.85",
    "@types/md5": "^2.1.32",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.53",
    "@types/sinon": "^4.0.0",
    "@types/sinon-chai": "^2.7.29",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-lodash": "^3.3.2",
    "chai": "^4.0.2",
    "codecov": "^3.0.0",
    "cross-env": "^5.0.1",
    "del": "^3.0.0",
    "fetch-mock": "^5.12.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0-beta.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-tslint": "^8.1.2",
    "gulp-typescript": "^3.2.3",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.6",
    "lodash-webpack-plugin": "^0.11.4",
    "merge-stream": "^1.0.1",
    "mocha": "^4.0.1",
    "nyc": "^11.0.3",
    "phantomjs-prebuilt": "^2.1.16",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.11.0",
    "source-map-support": "^0.5.0",
    "ts-loader": "^3.2.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "webpack-stream": "^4.0.0"
  }
}
