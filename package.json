{
  "name": "graphql-box",
  "description": "An extensible GraphQL client and server with modules for caching, request parsing, subscriptions and more.",
  "version": "5.0.6",
  "author": "Dylan Aubrey",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/badbatch/graphql-box",
  "repository": "badbatch/graphql-box",
  "bugs": "https://github.com/badbatch/graphql-box/issues",
  "type": "module",
  "engines": {
    "node": "^20.10.0",
    "pnpm": "^8.11.0"
  },
  "scripts": {
    "build": "pnpm run -r build",
    "clean:deps": "pnpm run -r clean:deps && del-cli ./node_modules",
    "clean:dist": "pnpm run -r clean:dist",
    "commit": "commit",
    "cut:changelog": "changelog",
    "cut:post-version": "pnpm run build",
    "lint": "eslint . --ext .ts,.tsx,.cjs",
    "repodog": "repodog",
    "syncpack": "syncpack format && syncpack list-mismatches && syncpack lint-semver-ranges",
    "test": "concurrently \"pnpm run test:browser\" \"pnpm run test:node\"",
    "test:browser": "pnpm run test:browser:remove && pnpm run test:browser:build && pnpm run test:browser:run",
    "test:browser:build": "webpack --config ./webpack.config.cjs",
    "test:browser:client": "wait-on tcp:3001 && jasmine-browser-runner runSpecs --config=jasmineBrowser.config.cjs --no-random",
    "test:browser:debug": "jasmine-browser-runner serve --config=jasmineBrowser.config.cjs --browser chrome",
    "test:browser:remove": "del-cli ./tests/browser/dist",
    "test:browser:run": "concurrently --kill-others --success first \"npm run test:browser:server\" \"npm run test:browser:client\"",
    "test:browser:server": "node --loader ts-node/esm ./tests/server/cli.ts",
    "test:node": "pnpm run /^test:node:.*/",
    "test:node:cjs": "BABEL_MODULE_SYSTEM=cjs node node_modules/jest/bin/jest.js --testMatch \"$(pwd)/packages/gql.macro/**/*.test.ts\"",
    "test:node:esm": "node --require=suppress-experimental-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --testMatch \"$(pwd)/packages/!(gql.macro)/**/*.test.ts\"",
    "type-check": "tsc --noEmit",
    "validate": "pnpm run syncpack && pnpm run build && pnpm run lint && pnpm run type-check && pnpm run test"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/plugin-transform-export-namespace-from": "^7.22.11",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.13",
    "@cachemap/core": "^5.0.8",
    "@cachemap/core-worker": "^5.0.8",
    "@cachemap/indexed-db": "^5.0.6",
    "@cachemap/map": "^5.0.6",
    "@cachemap/types": "^5.0.6",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@commitlint/prompt-cli": "^17.3.0",
    "@graphql-box/cache-manager": "workspace:*",
    "@graphql-box/client": "workspace:*",
    "@graphql-box/core": "workspace:*",
    "@graphql-box/debug-manager": "workspace:*",
    "@graphql-box/execute": "workspace:*",
    "@graphql-box/fetch-manager": "workspace:*",
    "@graphql-box/helpers": "workspace:*",
    "@graphql-box/request-parser": "workspace:*",
    "@graphql-box/server": "workspace:*",
    "@graphql-box/subscribe": "workspace:*",
    "@graphql-box/test-utils": "workspace:*",
    "@graphql-box/websocket-manager": "workspace:*",
    "@graphql-box/worker-client": "workspace:*",
    "@graphql-tools/schema": "^10.0.0",
    "@jest/globals": "^29.3.1",
    "@repodog/babel-config": "^1.1.11",
    "@repodog/cli": "^1.4.1",
    "@repodog/commitlint-config": "^1.1.6",
    "@repodog/eslint-config": "^1.1.19",
    "@repodog/eslint-config-jasmine": "^1.1.10",
    "@repodog/eslint-config-jest": "^1.1.10",
    "@repodog/eslint-config-react": "^1.1.10",
    "@repodog/jasmine-browser-config": "^0.1.0",
    "@repodog/jest-config": "^1.1.11",
    "@repodog/markdownlint-config": "^1.1.4",
    "@repodog/prettier-config": "^1.1.4",
    "@repodog/rollup-config": "^1.1.8",
    "@repodog/syncpack-config": "^1.2.3",
    "@repodog/ts-config": "^1.3.0",
    "@repodog/webpack-config": "^0.0.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jasmine": "^5.1.1",
    "@types/jest": "^25.1.3",
    "@types/lodash-es": "^4.14.149",
    "@types/node": "^20.9.0",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-codegen": "^4.1.5",
    "babel-plugin-macros": "^3.1.0",
    "body-parser": "^1.20.2",
    "cacheability": "^4.0.20",
    "concurrently": "^8.2.1",
    "core-js": "^3.27.2",
    "cors": "^2.8.5",
    "del-cli": "^5.1.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-class-members": "^1.16.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "eslint-plugin-unicorn": "^46.0.1",
    "express": "^4.17.1",
    "fetch-mock": "^9.11.0",
    "generate-changelog": "^1.8.0",
    "graphql": "^16.8.1",
