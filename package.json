{
  "name": "graphql-box",
  "version": "0.1.22",
  "description": "An extensible GraphQL client and server with modules for caching, request parsing, subscriptions and more.",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/badbatch/graphql-box",
  "bugs": {
    "url": "https://github.com/badbatch/graphql-box/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/badbatch/graphql-box"
  },
  "engines": {
    "node": "^10.16.0"
  },
  "keywords": [
    "graphql-box",
    "graphql",
    "client",
    "server",
    "isomorphic"
  ],
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "yarn run clean:libs && yarn run compile",
    "clean": "yarn run clean:packages && yarn run clean:libs && yarn run clean:docs && yarn run clean:repo",
    "clean:docs": "lerna exec --parallel -- del-cli ./docs",
    "clean:libs": "lerna exec --parallel -- del-cli ./lib",
    "clean:packages": "lerna exec --parallel -- del-cli ./node_modules",
    "clean:repo": "del-cli ./node_modules",
    "compile": "lerna run compile --stream",
    "compile:browser": "lerna run compile:browser --stream",
    "compile:main": "lerna run compile:main --stream",
    "compile:module": "lerna run compile:module --stream",
    "compile:types": "lerna run compile:types --stream",
    "cutoff": "cutoff-lerna",
    "integration": "concurrently --kill-others --success first \"yarn run integration:server\" \"yarn run integration:tests\"",
    "integration-update": "cross-env UPDATE=true PRUNE=true yarn run integration",
    "integration:tests": "cross-env BABEL_ENV=integration karma start --single-run --reporters mocha --browsers ChromeHeadlessNoSandbox",
    "integration:debug": "cross-env BABEL_ENV=integration karma start --no-single-run --reporters progress --browsers ChromeNoSandbox",
    "integration:server": "cross-env BABEL_ENV=main node ./integration/server/cli",
    "docs": "lerna run docs --parallel",
    "init": "yarn && yarn run bootstrap && yarn run build",
    "introspect:url": "node ./packages/cli/bin/introspect --url \"https://api.github.com/graphql\" --headers \"Authorization:bearer <TOKEN>\" --output \"test/introspections/github/index.json\"",
    "lint": "tslint --project ./tsconfig.json",
    "new-package": "new-package",
    "new-package:post": "yarn run bootstrap",
    "publish-cutoff": "publish-lerna-cutoff",
    "test": "jest --logHeapUsage",
    "type-check": "tsc --noEmit",
    "watch": "cross-env NODE_ENV=development lerna run watch --parallel",
    "wipe": "yarn run wipe:packages && yarn run wipe:repo",
    "wipe:packages": "lerna exec --parallel -- del-cli ./yarn.lock ./node_modules",
    "wipe:repo": "del-cli ./yarn.lock ./node_modules"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-sent": "^7.5.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "7.5.5",
    "@babel/runtime": "^7.5.5",
    "@cachemap/core": "^1.3.1",
    "@cachemap/indexed-db": "^1.3.1",
    "@cachemap/map": "^1.3.1",
    "@cachemap/redis": "^1.3.1",
    "@playlyfe/gql": "^2.6.1",
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.5",
    "@types/eventemitter3": "^2.0.2",
    "@types/express": "^4.17.0",
    "@types/fetch-mock": "^7.3.1",
    "@types/fs-extra": "^8.0.0",
    "@types/graphql": "^14.2.3",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.136",
    "@types/md5": "^2.1.33",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
    "@types/shelljs": "^0.8.5",
    "@types/sinon": "^7.0.13",
    "@types/uuid": "^3.4.5",
    "@types/ws": "^6.0.1",
    "@types/yargs": "^13.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.2",
    "cacheability": "^2.0.4",
    "chai": "^4.2.0",
    "chai-karma-snapshot": "^0.8.0",
    "concurrently": "^4.1.1",
    "core-js": "^2.6.8",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "cutoff": "^0.2.7",
    "del-cli": "^2.0.0",
    "dotenv": "^8.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "express": "^4.17.1",
    "fetch-mock": "^7.3.9",
    "graphql": "^14.4.2",
    "graphql-tools": "^4.0.5",
    "husky": "3.0.2",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.8.0",
    "karma": "^4.2.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-mocha-snapshot": "^0.2.1",
    "karma-sinon": "^1.0.5",
    "karma-snapshot": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "lerna": "^3.16.4",
    "lodash": "^4.17.15",
    "markdownlint-cli": "^0.17.0",
    "mocha": "^6.2.0",
    "mock-socket": "^9.0.0",
    "repodog": "^0.0.3",
    "rollup": "^1.17.0",
    "rollup-plugin-analyzer": "^3.1.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "sinon": "^7.3.2",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "type-fest": "^0.6.0",
    "typedoc": "^0.15.0",
    "typedoc-plugin-markdown": "^2.1.0",
    "typescript": "^3.5.3",
    "webpack": "^4.38.0",
    "ws": "^7.1.1"
  }
}
