    "jasmine-browser-runner": "^2.3.0",
    "jasmine-core": "^5.1.1",
    "jasmine-expect": "^5.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lodash-es": "^4.17.15",
    "markdownlint-cli": "^0.37.0",
    "prettier": "^2.8.1",
    "rollup": "^3.7.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "source-map-loader": "^4.0.1",
    "suppress-experimental-warnings": "^1.1.3",
    "syncpack": "^12.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.3",
    "uuid": "^9.0.1",
    "wait-on": "^7.2.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "ws": "^8.14.2"
  },
  "keywords": [
    "client",
    "graphql",
    "graphql-box",
    "isomorphic",
    "server"
  ]
}