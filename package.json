{
  "name": "handl",
  "version": "0.0.28",
  "description": "An isomorphic graphql client and cache.",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/dylanaubrey/handl",
  "bugs": {
    "url": "https://github.com/dylanaubrey/handl/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanaubrey/handl"
  },
  "main": "lib/main/index.js",
  "module": "lib/module/index.js",
  "browser": "lib/browser/index.js",
  "types": "lib/main/index.d.ts",
  "keywords": [
    "handl",
    "graphql",
    "client",
    "isomorphic"
  ],
  "scripts": {
    "analyze:bundle": "webpack-bundle-analyzer bundle/stats.json lib/umd --mode static --report bundle/report.html --no-open",
    "build": "npm run clean && npm run test && npm run compile && npm run analyze:bundle",
    "clean": "gulp clean",
    "compile": "npm run compile:main && npm run compile:module && npm run compile:browser && npm run compile:umd",
    "compile:browser": "cross-env BABEL_ENV=browser gulp browser",
    "compile:main": "cross-env BABEL_ENV=main gulp main",
    "compile:module": "cross-env BABEL_ENV=module gulp module",
    "compile:umd": "cross-env BABEL_ENV=browser gulp umd",
    "debug:web": "cross-env BABEL_ENV=debug TEST_ENV=true karma start --no-single-run --browsers Chrome",
    "lint": "gulp lint",
    "publish:pkg": "npm run build && npm publish",
    "test": "npm run lint && npm run type-check && npm run test:web && npm run test:node",
    "test:node": "cross-env TS_NODE_PROJECT=\"./tsconfig.test.json\" nyc mocha --opts \"./mocha.opts\"",
    "test:web": "cross-env BABEL_ENV=browser TEST_ENV=true karma start --single-run --browsers ChromeHeadless --reporters mocha,coverage-istanbul",
    "type-check": "gulp type-check"
  },
  "dependencies": {
    "cacheability": "0.0.49",
    "cachemap": "0.0.41",
    "defer-promise": "^1.0.1",
    "graphql": "^0.12.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "promise-worker": "^1.1.1",
    "ws": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.36",
    "@babel/preset-env": "^7.0.0-beta.36",
    "@babel/preset-stage-0": "^7.0.0-beta.36",
    "@types/chai": "^4.0.8",
    "@types/defer-promise": "^1.0.0",
    "@types/fetch-mock": "^5.12.2",
    "@types/graphql": "^0.11.6",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/lodash": "^4.14.85",
    "@types/md5": "^2.1.32",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.53",
    "@types/redis": "^2.8.3",
    "@types/sinon": "^4.0.0",
    "@types/sinon-chai": "^2.7.29",
    "@types/ws": "^3.2.1",
    "awesome-typescript-loader": "^3.4.1",
    "babel-eslint": "^8.0.2",
    "babel-plugin-lodash": "^3.3.2",
    "chai": "^4.0.2",
    "codecov": "^3.0.0",
    "cross-env": "^5.0.1",
    "del": "^3.0.0",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "fakeredis": "^2.0.0",
    "fetch-mock": "^5.12.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0-beta.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-tslint": "^8.1.2",
    "gulp-typescript": "^3.2.3",
    "istanbul-instrumenter-loader": "^3.0.0",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.6",
    "lodash-webpack-plugin": "^0.11.4",
    "merge-stream": "^1.0.1",
    "mocha": "^4.0.1",
    "nyc": "^11.0.3",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.11.0",
    "source-map-loader": "^0.2.3",
    "source-map-support": "^0.5.0",
    "ts-node": "^4.0.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-stream": "^4.0.0",
    "worker-loader": "^1.1.0"
  }
}
